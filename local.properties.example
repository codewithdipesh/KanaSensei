# Copy this file to local.properties and fill in the values
# local.properties is gitignored and will not be committed

# Android SDK location (auto-generated by Android Studio)
sdk.dir=/path/to/your/android/sdk

# Google OAuth Web Client ID
# Get this from: Firebase Console -> Your Project -> Authentication -> Sign-in method -> Google -> Web SDK configuration -> Web client ID
WEB_CLIENT_ID=your-web-client-id.apps.googleusercontent.com
